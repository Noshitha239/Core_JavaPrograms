package com.gqt.StringinterviewQuestions;
import java.util.*;
public class Sai27 {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter n value: ");
		int n=sc.nextInt();
		// TODO Auto-generated method stub
		for(int i=1;i<=n;i++) {
			for(int j=0;j<=i;j++){
				
				System.out.print((i+j)%2);
			}
			System.out.println();
		}
	}

}
