import java.util.*;
public class Arraysum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int m=sc.nextInt();
        int o=sc.nextInt();
        int a1[][][]=sc.new int[n][m][o];
        int b1[][][]=sc.new int[n][m][o];
        int c1[][][]=sc.new int[n][m][o];
        for(int i=0;i<=a1.length;i++) {
        	for(int j=0;j<=a1[i].length;i++) {
        	    for(int k=0;k<=a1[i][j].length;k++) {
        	    	
                	a1[i][j][k]=sc.nextInt();
        	    }
        	}
        }
        for(int i=0;i<=b1.length;i++) {
        	for(int j=0;j<=b1[i].length;i++) {
        	    for(int k=0;k<=b1[i][j].length;k++) {
        	    	
                	b1[i][j][k]=sc.nextInt();
        	    }
        	}
        }
        for(int i=0;i<=c1.length;i++) {
        	for(int j=0;j<=c1[i].length;i++) {
        	    for(int k=0;k<=c1[i][j].length;k++) {
                	c1[i][j][k]=a1[i][j][k];
        	    }
        	}
        }
        for(int i=0;i<=c1.length;i++) {
        	for(int j=0;j<=c1[i].length;i++) {
        	    for(int k=0;k<=c1[i][j].length;k++) {
                	System.out.println(c1[i][j][k]);
        	    }
        	}
        }
        
	}

}
